
.d-icon-item{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 33.33%;
  margin-top: 40rpx;
}
.d-icon-name{
  font-size: 20rpx;
  margin-top: 8rpx;
}

.d-longFocusInput-wrap{
  position: relative;
  background-color: #fff;
  overflow: hidden;
  border-radius: 40rpx;

}
.custom-class-input {
  border: 2rpx solid #00A19C;
  height: 74rpx;
  padding-left: 40rpx;
  padding-right: 150rpx;
  border-radius: 40rpx;
}
.custom-class-slot {
  position: absolute;
  background: #ECECEC;
  top: 16rpx !important;
  right: 88rpx;
  left: unset !important;
  bottom: unset !important;
  border-radius: 50%;
  overflow: hidden;
}
.search{
  position: absolute;
  top: 8rpx;
  right: 10rpx;
}