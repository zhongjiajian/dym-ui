<pageCard pageName="flyToCart">
  <exampleCard title="Doc" des='{{example1Des}}'>
      <d-button catchtap="addToCart">+ 加入购物车</d-button>
      <space/>
      <d-button catchtap="addArcRate">+ 增加弧度</d-button>
      <space/>
      <d-button catchtap="reduceArcRate">- 减少弧度</d-button>
      <space/>
      <text>当前弧度：{{arcRate}}</text>
      <space/>
    <image class="cart" src="/images//cart.png" alt="购物车" style="left:{{cartLeft}}%;" />
  </exampleCard>
  <exampleCard title="移动购物车" >
    <slider block-size="{{20}}" bindchanging="sliderChangeing" />
  </exampleCard>
  <space height="800" />
</pageCard>
<d-flyToCart class="flyToCart" endPosition="{{cartPosition}}" arcRate="{{arcRate}}" />
